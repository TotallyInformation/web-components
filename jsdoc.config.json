{
    "source": {
        "include": ["src"],
        "xx_includePattern": "component-template.mjs",
        "includePattern": ".+\\..js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs/jsdoc",
        "recurse": true,
        "readme": "./README.md"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}
